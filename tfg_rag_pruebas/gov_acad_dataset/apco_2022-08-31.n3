apco:faitUneOffre  a  owl:AsymmetricProperty , owl:ObjectProperty ;
        rdfs:comment  "Proprieté indiquant l'offre faite par un soumissionnaire. (cardinalité 0..1)"@fr , "Property indicating the offer made by a bidder. (cardinality 0..1)"@en ;
        rdfs:domain   apco:Soumissionnaire ;
        rdfs:label    "make an Offer"@en , "fait ne Offre"@fr ;
        rdfs:range    apco:Offre .

apco:Soumissionnaire  a  owl:Class ;
        rdfs:comment     "Personne physique aui fait une offre à l'endroit de l'autorité contractante"@fr , "Natural person who made a bid to the contracting authority"@en ;
        rdfs:label       "Tenderer"@en , "Soumissionnaire"@fr ;
